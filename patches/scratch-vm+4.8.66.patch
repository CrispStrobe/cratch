+        if (!this.isConnected()) return Promise.resolve(this);
+        if (this.bleBusy) {
+            return Promise.resolve(this);
+        }
+        this.bleBusy = true;
+        this.bleBusyTimeoutID = window.setTimeout(() => {
+            this.bleBusy = false;
